/* pages/video/video.wxss */
.section{
  width:96%;
  margin:30rpx 2% 5rpx 2%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
}
.section-item{
  width:49%;
  margin-top:20rpx;
}
.images{
  width:100%;
  height:262rpx;
}
.time{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.count{
  width:50%;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.date{
  width:50%;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.text{
  font-size:24rpx;
}