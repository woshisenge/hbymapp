/* pages/analog/analogs/analogs.wxss */
page{
  background-color:#EEEEEE;
}
.head{
  width:100%;
  height:240rpx;
}
.head image{
  width:100%;
  height:100%;
}
.content1{
  width:94%;
  height:160rpx;
  background-color:white;
  margin:1% 3%;
  border-radius: 10rpx;
  font-size:28rpx;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
}
.content1-left{
  display: flex;
  flex-direction: column;
  font-size:30rpx;
  margin:45rpx 30rpx;
}
.content1-right{
  display: flex;
  flex-direction: column;
  align-items:center;
  margin:30rpx 30rpx;
}
.content2{
  width:94%;
  margin:0 3%;
  font-size:30rpx;
  display: flex;
  flex-direction: row;
}
.content2 text{
  display: flex;
  align-items:center;
  justify-content: center;
}
.weui-cells{
  font-size:28rpx;
}
.content4{
  width:94%;
  margin:8% 3%;
}
.footer{
  width:94%;
  margin:100rpx 3% 0 3%;
  border-radius: 10rpx;
  border:1rpx solid #999999;  
  font-size:20rpx;
}
.footer1{
  padding:0 20rpx;
}
.footer1 text{
  padding:0 10rpx;
  font-size:28rpx;
}
.footer2{
  padding:20rpx 20rpx 0 20rpx;
}