<view class="page_bd">
    <view class="banner">
      <image src='/images/tb.png'></image>
    </view>
    <view class="top">
      <view class="top-left">
        <image src="{{logo}}" style='width:170rpx;height:170rpx;border-radius:50%'></image>
      </view>
      <view class="top-right">
        <view class='top-right-1'>
          <text>{{name}}</text>
          <image wx:for="{{properties}}" wx:key="" src="/images/icon/{{item.DIC_ID}}.png" style='width:35rpx;height:35rpx;margin-right:15rpx;'></image>
        </view>
        <view class='top-right-2'>
          <text>{{types}}</text>
          <text>{{region}}</text>
          <image wx:for="{{subjecttypes}}" wx:key="" src="/images/icon/{{item.DIC_ID}}.png" style='width:80rpx;height:40rpx;margin-right:15rpx;'></image>
        </view>
      </view>
    </view>
    <view class="page__bd page__bd_spacing">
        <view class="weui-flex">
            <view class="weui-flex__item">
              <view class="placeholder" style='text-align:left'>院校历年数据：</view></view>
              <view class="weui-flex__item"><view class="placeholder" style='text-align:right'>{{style}}　{{subject}}</view>
            </view>
        </view>
        <view class="weui-flex">
            <view class="weui-flex__item"><view class="placeholder">年份</view></view>
            <view class="weui-flex__item"><view class="placeholder">最高分</view></view>
            <view class="weui-flex__item"><view class="placeholder">最低分</view></view>
            <view class="weui-flex__item"><view class="placeholder">录取人数</view></view>
        </view>
        
        <view class="weui-flex">
            <view class="weui-flex__item"><view class="placeholder" wx:for="{{grade}}" wx:key="" hidden='{{item.checked}}'>{{item.YEAR_VALUE}}</view></view>
            <view class="weui-flex__item"><view class="placeholder" wx:for="{{grade}}" wx:key="" hidden='{{item.checked}}'> {{item.MAXSCORE}}</view></view>
            <view class="weui-flex__item"><view class="placeholder" wx:for="{{grade}}" wx:key="" hidden='{{item.checked}}'>{{item.MINSCORE}}</view></view>
            <view class="weui-flex__item"><view class="placeholder" wx:for="{{grade}}" wx:key="" hidden='{{item.checked}}'>{{item.ENTERCOUNT}}</view></view>
        </view>
    </view>
    <view style="background-color:white;margin-top:20rpx">
    <view class="page__bd page__bd_spacing">
      <view class="weui-flex">
            <view class="weui-flex__item"><view class="placeholder" style='text-align:left'>专业招生计划及历年数据：</view></view>
      </view>
    </view>
    <view class='whole'>
      <view class='left'><text>招生专业</text></view>
      <view class='center'>
        <view class="page__bd page__bd_spacing">
          <view class="weui-flex">
            <view class="weui-flex__item">
              <view class="placeholder">专业历年招生统计</view>
            </view>
          </view>
          <view class="weui-flex">
            <view class="weui-flex__item2"><view class="placeholder">年份</view></view>
            <view class="weui-flex__item1"><view class="placeholder">分数</view></view>
            <view class="weui-flex__items"><view class="placeholder">排名</view></view>
            <view class="weui-flex__item2"><view class="placeholder">录取</view></view>
        </view>
        </view>
      </view>
      <view class='right'><text>概率</text></view>
    </view>
    <view wx:for="{{results}}" wx:key="*this" class='whole'>
      <view class='left'><text>{{item.MJNAME}}</text></view>
      <view class='center'>
        <view class="page__bd page__bd_spacing" wx:for="{{item.scores}}" wx:for-item="score" wx:key="">
          <view class="weui-flex">
            <view class="weui-flex__item2 year"><view class="placeholder" >{{score.YEAR_VALUE}}</view></view>
            <view class="weui-flex__item1 year"><view class="placeholder" >{{score.MINSCORE}}~{{score.MAXSCORE}}</view></view>
            <view class="weui-flex__items year"><view class="placeholder" >{{score.MAXSCORETOTALCOUNT}}~{{score.MINSCORETOTALCOUNT}}</view></view> 
            <view class="weui-flex__item2 year"><view class="placeholder">{{score.MSORT}}</view></view>     
        </view>  
        </view>
      </view>
      <view class="right"><text>{{item.chance}}</text></view>
    </view>
  </view>
  </view>
  <view class='white'></view>