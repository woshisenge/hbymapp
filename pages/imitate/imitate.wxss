/* pages/analog/analogs/analogs.wxss */
page{
  background-color:#EEEEEE;
}
.content1{
  width:100%;
  height:160rpx;
  background-color:white;
  border-radius: 10rpx;
  font-size:28rpx;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  margin-bottom: 15rpx;
}
.content1-left{
  display: flex;
  flex-direction: column;
  font-size:30rpx;
  margin:30rpx 30rpx;
}
.content1-right{
  display: flex;
  flex-direction: column;
  align-items:center;
  margin:30rpx 30rpx;
}
.content2{
  width:100%;
  padding:0 3%;
  font-size:30rpx;
  display: flex;
  flex-direction: row;
  background-color:white;
}
.content2 text{
  display: flex;
  align-items:center;
  justify-content: center;
}
.weui-cells{
  font-size:28rpx;
}
.content4{
  width:94%;
  margin:8% 3%;
 
}
.footer{
  width:94%;
  margin:15rpx 3% 0 3%;
  border-radius: 10rpx;
  border:1rpx solid #999999;  
  font-size:25rpx;
   background-color:white;
}
.footer1{
  padding:20rpx 20rpx 0 25rpx;
  display: flex;
  flex-direction: row;
  justify-content:flex-start;
  align-items: center;
}
.footer1 text{
  padding:0 10rpx;
  font-size:28rpx;
}
.footer2{
  padding:20rpx 20rpx 0 20rpx;
}
.weui-cells:before {
  border-top:0;
}
.weui-navbar{
  background-color:white;
}
.blank{
  width:100%;
  height:50rpx;
}